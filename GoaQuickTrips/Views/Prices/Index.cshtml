@model IEnumerable<GoaQuickTrips.Price>

@{
    ViewBag.Title = "Price Details";
}

<h2>@ViewBag.Title</h2>
<p>
    @Html.ActionLink("Create New", "Create", new { id = (int)ViewBag.ApartmentID })
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WEF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apartment.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.WEF)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apartment.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PriceID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PriceID })
        </td>
    </tr>
}

</table>

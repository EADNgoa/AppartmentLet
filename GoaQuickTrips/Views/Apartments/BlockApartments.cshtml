@model IEnumerable<GoaQuickTrips.Apartment>

@{
    ViewBag.Title = "Index";
}
<style>

</style>

<h2>Index</h2>
<div class="container">
    <table class="table">
        <tr>
            <th>
                Name
            </th>
            <th>
               Description
            </th>
            <th>
               Rules
            </th>
            <th>
              Notes
            </th>
            <th>
               No Of Guests
            </th>
            <th>
                Address
            </th>
            <th>
               Email
            </th>
            <th>
             Phone
            </th>
            <th>
               Location Info:
            </th>
            <th>
               Cancellation Policy
            </th>
            <th>
               Lat
            </th>
            <th>
                Lang
            </th>
         
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Rules)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Notes)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NoOfGuests)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Phone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LocationInfo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CancellationPolicy)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Lat)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Lang)
                </td>
              
                <td>
                    @Html.ActionLink("Block", "AddBlockDates", new { id = item.ApartmentID }) |
                   
                </td>
            </tr>
        }

    </table>
</div>
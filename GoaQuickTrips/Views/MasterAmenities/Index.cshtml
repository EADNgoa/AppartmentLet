@model IEnumerable<GoaQuickTrips.MasterAmenity>

@{
    ViewBag.Title = "Amenities";
}

<h2>Amenities</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Amenity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Amenity)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MasterID }) 
           
        </td>
    </tr>
}

</table>
